{
    "name": "hamba/queryget",
    "description": "Helper for query eloquent model from request",
    "authors": [
        {
            "name": "fatkhank",
            "email": "fatkhan.khoirurridlo@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Hamba\\QueryGet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hamba\\QueryGet\\QueryGetServiceProvider"
            ],
            "aliases": {
                "QG": "Hamba\\QueryGet\\QG",
                "Filterable": "Hamba\\QueryGet\\Filterable",
                "Selectable": "Hamba\\QueryGet\\Selectable",
                "Sortable": "Hamba\\QueryGet\\Sortable"
            }
        }
    }
}
